{
  "name": "@whoj/sakai-nuxt-theme",
  "type": "module",
  "version": "1.1.1",
  "packageManager": "pnpm@7.26.3",
  "homepage": "https://github.com/who-jonson/sakai-nuxt-theme#readme",
  "repository": {
    "url": "https://github.com/who-jonson/sakai-nuxt-theme",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/who-jonson/sakai-nuxt-theme/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "main": "./nuxt.config.ts",
  "files": [
    "assets",
    "components",
    "composables",
    "directives",
    "layouts",
    "modules",
    "pages",
    "plugins",
    "public",
    "services",
    ".postcssrc",
    "app.config.ts",
    "nuxt.config.ts",
    "app.vue",
    "error.vue",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": "^18.12"
  },
  "scripts": {
    "dev": "nuxi prepare & nuxi dev .playground",
    "build": "nuxi build .playground",
    "generate": "nuxi generate .playground",
    "preview": "nuxi preview .playground",
    "stub": "nuxi prepare & nuxi prepare .playground",
    "release": "npm run lint:fix && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.5",
    "@vueuse/components": "^9.12.0",
    "@vueuse/nuxt": "^9.12.0",
    "@whoj/utils": "^1.5.0",
    "chart.js": "4.2.0",
    "pinia": "^2.0.30",
    "primeflex": "^3.2.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.22.1",
    "prismjs": "1.29.0",
    "sass": "^1.58.0"
  },
  "devDependencies": {
    "@nuxt/content": "^2.2.2",
    "@nuxt/test-utils": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@pinia/testing": "^0.0.14",
    "@testing-library/vue": "latest",
    "@types/node": "^18.11.18",
    "@types/prismjs": "^1.26.0",
    "@types/serialize-javascript": "^5.0.2",
    "@whoj/eslint-config": "^1.2.0",
    "changelogen": "^0.4.1",
    "eslint": "^8.33.0",
    "nuxt": "^3.1.2",
    "pinceau": "^0.13.8",
    "typescript": "^4.9.5"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "stable": "*"
    },
    "overrides": {
      "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@latest"
    }
  }
}
